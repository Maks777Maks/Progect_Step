<Window x:Class="WpfApplication2_Template_.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication2_Template_"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ComboBox Grid.Row="1" Name="combo" Height="80" Width="200"/>
        <Button Template="{StaticResource BtnTemplate}" Content="Hello!!!" Height="70" Width="200"/>
        <CheckBox  Grid.Row="2" Checked="IsCheked" Name="check" Unchecked="check_Unchecked"/>
    </Grid>
</Window>
