<Window x:Class="Tic_tac_toe_UDP_.Winner"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tic_tac_toe_UDP_"
        mc:Ignorable="d"
        Title="Winner" Height="300" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="225*"/>
            <RowDefinition Height="44*"/>
        </Grid.RowDefinitions>
        <Label Grid.ColumnSpan="2">
            <Image Source="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQ5qNpPBpT0a_wTj12-o4eQgjRfCm_J-qOOqgMJBn5S29SMNKKcYQ"/>
        </Label>
        <Button Grid.Column="1" Grid.Row="1" Content="OK" Click="Button_Click"/>
    </Grid>
</Window>
